#include <iostream>
#include <windows.h>
#include <string>
using namespace std;
int main()
{
	setlocale(LC_ALL, "Russian");
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);
	char ch(32);
	string S;
	int i, N, j, m=0, t;
	cout << "Введите строку:\n";
	getline(cin, S);
	N = S.length();
	t = S.find(ch);
	for (i = t + 1; i <= N; i++)
		cout << S[i];
	return 0;
}
