#include <iostream>
#include <windows.h>
#include <string>
using namespace std;
int main()
{
	setlocale(LC_ALL, "Russian");
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);
	string S;
	int i,N;
	cout << "Введите строку:\n";
	getline(cin, S);
	N = S.length();
	for (i = 0; i <= (2 * N); i++)
		cout << S[i] << " ";
	return 0;
}

